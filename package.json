{
    "name": "parking-lot-app-api",
    "version": "5.5.0",
    "description": "Ack NestJs Boilerplate",
    "repository": {
        "type": "git",
        "url": "https://github.com/andrechristikan/parking-lot-app-api.git"
    },
    "author": {
        "name": "Cornal",
        "email": "duy.nguyen@user.com.sg"
    },
    "private": true,
    "license": "MIT",
    "scripts": {
        "typeorm": "typeorm-ts-node-esm",
        "upgrade:package": "ncu -u",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "yarn format:src && yarn format:test",
        "format:src": "prettier --write src/**/*.ts",
        "format:test": "prettier --write test/**/*.ts",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/src/main",
        "lint": "yarn lint:src && yarn lint:test",
        "lint:fix": "eslint --ext .ts,.tsx '{src,test}/**/*.ts' --fix --no-error-on-unmatched-pattern",
        "lint:src": "eslint --ext .ts,.tsx 'src/**/*.ts' --no-error-on-unmatched-pattern",
        "lint:test": "eslint --ext .ts,.tsx 'test/**/*.ts' --no-error-on-unmatched-pattern",
        "test": "TZ=UTC jest --config test/jest.json --runInBand --passWithNoTests --forceExit",
        "prepare": "husky install",
        "deadcode": "ts-prune --project tsconfig.json --unusedInModule --skip *.json",
        "deadcode:filter": "ts-prune --project tsconfig.json --unusedInModule --skip *.json | grep -v '(used in module)'",
        "deadcode:count": "ts-prune --project tsconfig.json --unusedInModule --skip *.json | grep -v '(used in module)' | wc -l",
        "spell": "yarn spell:src && yarn spell:test",
        "spell:src": "cspell lint --config cspell.json src/**/*.ts --color --gitignore --no-must-find-files --no-summary --no-progress || true",
        "spell:test": "cspell lint --config cspell.json test/**/*.ts --color --gitignore --no-must-find-files --no-summary --no-progress || true",
        "migration:generate": "yarn run typeorm migration:generate -d ormconfig",
        "migration:create": "yarn run typeorm migration:create -d ormconfig",
        "migration:run": "yarn run typeorm migration:run -d ormconfig",
        "seed:parkingLot": "nestjs-command seed:parkingLot",
        "seed:parkingSpot": "nestjs-command seed:parkingSpot",
        "rollback:parkingLot": "nestjs-command remove:parkingLot",
        "rollback:parkingSpot": "nestjs-command remove:parkingSpot",
        "seed": "yarn seed:parkingLot && yarn seed:parkingSpot",
        "rollback": "yarn rollback:parkingSpot && yarn seed:parkingLot"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.423.0",
        "@aws-sdk/client-ses": "^3.427.0",
        "@casl/ability": "^6.5.0",
        "@faker-js/faker": "^8.1.0",
        "@joi/date": "^2.1.0",
        "@nestjs/axios": "^3.0.0",
        "@nestjs/bull": "^10.0.1",
        "@nestjs/common": "^10.2.6",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.2.6",
        "@nestjs/cqrs": "^10.0.1",
        "@nestjs/event-emitter": "^2.0.2",
        "@nestjs/jwt": "^10.1.1",
        "@nestjs/passport": "^10.0.2",
        "@nestjs/platform-express": "^10.2.6",
        "@nestjs/schedule": "^3.0.4",
        "@nestjs/swagger": "^7.1.12",
        "@nestjs/terminus": "^10.1.1",
        "@nestjs/throttler": "^5.0.0",
        "@nestjs/typeorm": "^10.0.0",
        "@types/mustache": "^4.2.3",
        "@types/response-time": "^2.3.6",
        "aws-sdk": "^2.1472.0",
        "axios": "^1.5.1",
        "bcryptjs": "^2.4.3",
        "bull": "^4.11.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "crypto-js": "^4.1.1",
        "helmet": "^7.0.0",
        "joi": "^17.10.2",
        "minio": "^7.1.3",
        "moment": "^2.29.4",
        "morgan": "^1.10.0",
        "mustache": "^4.2.0",
        "nest-winston": "^1.9.4",
        "nestjs-command": "^3.1.4",
        "nestjs-i18n": "^10.3.5",
        "passport": "^0.6.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-headerapikey": "^1.2.2",
        "passport-jwt": "^4.0.1",
        "pg": "^8.11.1",
        "reflect-metadata": "^0.1.13",
        "response-time": "^2.3.2",
        "rimraf": "^5.0.5",
        "rotating-file-stream": "^3.1.1",
        "rxjs": "^7.8.1",
        "swagger-themes": "^1.2.30",
        "typeorm": "^0.3.17",
        "typeorm-transactional": "^0.4.1",
        "ua-parser-js": "^1.0.36",
        "winston": "^3.10.0",
        "winston-daily-rotate-file": "^4.7.1",
        "xlsx": "^0.18.5",
        "yargs": "^17.7.2",
        "yarn": "^1.22.19"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.1.18",
        "@nestjs/schematics": "^10.0.2",
        "@nestjs/testing": "^10.2.6",
        "@types/bcryptjs": "^2.4.4",
        "@types/bytes": "^3.1.2",
        "@types/cors": "^2.8.14",
        "@types/cron": "^2.0.1",
        "@types/crypto-js": "^4.1.2",
        "@types/express": "^4.17.18",
        "@types/jest": "^29.5.5",
        "@types/lodash": "^4.14.199",
        "@types/morgan": "^1.9.6",
        "@types/ms": "^0.7.32",
        "@types/multer": "^1.4.8",
        "@types/node": "^20.8.2",
        "@types/passport-google-oauth20": "^2.0.12",
        "@types/passport-jwt": "^3.0.10",
        "@types/supertest": "^2.0.14",
        "@types/ua-parser-js": "^0.7.37",
        "@types/uuid": "^9.0.4",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "cspell": "^7.3.7",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "prettier": "^3.0.3",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "ts-prune": "^0.10.3",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2"
    }
}
